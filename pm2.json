 {
  "apps": {
    "name": "webapp",
    "script": "./dist/app.js",
    "error_file": ".temp/logs/app-err.log",
    "out_file": ".temp/logs/app-out.log",
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "merge_logs": true,
    "max_memory_restart": "1G",
    "exec_mode": "cluster",
    "instances": 2,
    "env": {
      "NODE_ENV": "production"
    },
    "env_development": {
      "NODE_ENV": "development"
    },
    "env_testing": {
      "NODE_ENV": "testing"
    }
  }
}